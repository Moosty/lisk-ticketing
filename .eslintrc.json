{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "parser": "@babel/eslint-parser",
  "plugins": [
    "jsx-a11y",
    "prettier",
    "@babel"
  ],
  "rules": {
    "requireConfigFile": 0,
    "semi": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
//    "requireConfigFile": false
  }
}
